@article{doi:10.1162isal_a_00159,
author = {Raimbault, Juste and Perret, Julien},
title = {Generating urban morphologies at large scales},
journal = {The 2019 Conference on Artificial Life},
volume = {},
number = {31},
pages = {179-186},
year = {2019},
doi = {10.1162/isal\_a\_00159},

URL = { 
        https://www.mitpressjournals.org/doi/abs/10.1162/isal_a_00159
    
},
eprint = { 
        https://www.mitpressjournals.org/doi/pdf/10.1162/isal_a_00159
    
}
,
    abstract = { At large scales, typologies of urban form and corresponding generating processes remain an open question with important implications regarding urban planning policies and sustainability. We propose in this paper to generate urban configurations at large scales, typically of districts, with morphogenesis models, and compare these to real configurations according to morphological indicators. Real values are computed on a large sample of districts taken in European urban areas. We calibrate each model and show their complementarity to approach the variety of real urban configurations, paving the way to multi-model approaches of urban morphogenesis. }
}




